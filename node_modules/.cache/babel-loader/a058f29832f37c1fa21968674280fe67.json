sourcesContent":["<template>\n  <div>\n    <!-- <ElementTest></ElementTest> -->\n\n    <!-- KForm -->\n    <KForm :model=\"userInfo\" :rules=\"rules\" ref=\"loginForm\">\n      <!-- 用户名 -->\n      <KFormItem label=\"用户名\" prop=\"username\">\n        <KInput v-model=\"userInfo.username\" placeholder=\"请输入用户名\"></KInput>\n      </KFormItem>\n      <!-- 密码 -->\n      <KFormItem label=\"密码\" prop=\"password\">\n        <KInput type=\"password\" v-model=\"userInfo.password\" placeholder=\"请输入用户名\"></KInput>\n      </KFormItem>\n      <!-- 提交按钮 -->\n      <KFormItem>\n        <button @click=\"login\">登录</button>\n      </KFormItem>\n    </KForm>\n  </div>\n</template>\n\n<script>\nimport ElementTest from \"@/components/form/ElementTest.vue\";\nimport KInput from \"@/components/form/KInput.vue\";\nimport KFormItem from \"@/components/form/KFormItem.vue\";\nimport KForm from \"@/components/form/KForm.vue\";\n// import Notice from \"@/components/Notice.vue\";\n\nexport default {\n  data() {\n    return {\n      userInfo: {\n        username: \"tom\",\n        password: \"\"\n      },\n      rules: {\n        username: [{ required: true, message: \"请输入用户名称\" }],\n        password: [{ required: true, message: \"请输入密码\" }]\n      }\n    };\n  },\n  components: {\n    ElementTest,\n    KInput,\n    KFormItem,\n    KForm\n  },\n  methods: {\n    login() {\n      this.$refs[\"loginForm\"].validate(valid => {\n        const notice = this.$notice({\n          title: \"社会你杨哥喊你来搬砖\",\n          message: valid ? \"请求登录!\" : \"校验失败!\",\n          duration: 2000\n        });\n        notice.show();\n        // if (valid) {\n        //   alert(\"submit\");\n        // } else {\n        //   console.log(\"error submit!\");\n        //   return false;\n        // }\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n</style>"],"sourceRoot":"src/components/form"}]}